---
import ServiceLayout from '../../../components/layout/service_page.astro';
import Base from '../../../components/layout/base.astro';
import { getEntry } from 'astro:content';

const entry = await getEntry("servicePage", 'content/index');

if (!entry) {
  throw new Error("index.md not found in 'homepage' collection");
}
const indexpro = entry.data;
---

<Base>
<ServiceLayout indexpro={indexpro} /> 
</Base>
